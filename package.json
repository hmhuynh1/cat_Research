{
	"name": "about-cat",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"serve": "nodemon -x \"node server.cjs\"",
		"dev:client": "npx cross-env VITE_SERVER_URL=\"http://localhost:3000/\" npx vite --host 0.0.0.0",
		"dev": "concurrently \"npm run dev:client\" \"npm run serve\"",
		"build": "npx cross-env VITE_SERVER_URL=\"/\" tsc -b && VITE_SERVER_URL=\"/\" vite build",
		"start": "serve dist",
		"lint": "eslint . --ext ts,tsx --fix",
		"preview": "vite preview"
	},
	"dependencies": {
		"@chakra-ui/react": "^2.8.2",
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"framer-motion": "^11.3.2",
		"localforage": "^1.10.0",
		"match-sorter": "^6.3.4",
		"mongodb": "^6.8.0",
		"mongoose": "^8.4.4",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.24.1",
		"serve": "^14.2.3",
		"sort-by": "^0.0.2"
	},
	"devDependencies": {
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.13.1",
		"@typescript-eslint/parser": "^7.13.1",
		"@vitejs/plugin-react": "^4.3.1",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.0",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.7",
		"nodemon": "^3.1.4",
		"typescript": "^5.2.2",
		"vite": "^5.3.1"
	}
}
